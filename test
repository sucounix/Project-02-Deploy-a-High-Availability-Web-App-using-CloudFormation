aws ec2 describe-instances --region 'us-west-2' --region 'us-west-2' --query 'Reservations[].Instances[].[LaunchTime,InstanceId,InstanceType,Placement.AvailabilityZone, Tags[?Key==`Name`].Value|[0], State.Name]' | jq '.[]' --compact-output | sed 's/\[\|\]//g'
